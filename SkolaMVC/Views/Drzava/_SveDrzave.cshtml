@model IEnumerable<SkolaMVC.Models.DrzavaViewModel>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Drzavaid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Naziv)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Drzavaid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Naziv)
        </td>    
    </tr>
}

</table>
